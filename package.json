{
  "name": "nuxt-demo",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Anass Ez-Zouaine <ans-ezzouaine@hotmail.com>",
  "private": true,
  "engines": {
    "yarn": "1.7.0",
    "nuxt": "1.4.1"
  },
  "scripts": {
    "ddev": "nuxt -c config/nuxt.desktop.config.js",
    "dbuild": "nuxt build -c config/nuxt.desktop.config.js",
    "dstart": "nuxt start  -c config/nuxt.desktop.config.js",
    "dgenerate": "nuxt generate nuxt -c config/nuxt.desktop.config.js",
    "mdev": "nuxt -c config/nuxt.mobile.config.js",
    "mbuild": "nuxt build -c config/nuxt.mobile.config.js",
    "mstart": "nuxt start -c config/nuxt.mobile.config.js",
    "mgenerate": "nuxt generate -c config/nuxt.mobile.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "nuxt": "^1.0.0",
    "nuxt-device-detect": "^1.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
